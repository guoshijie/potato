{
    "name": "shinc/MarketWeb",
    "description": "Market Web",
    "type": "project",
    "version": "1.0",
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "http://packagist.phpcomposer.com"
        },
        "commons": {
            "type": "path",
            "url": "../commons"
        },
        "AutoID": {
            "type": "path",
            "url": "../AutoID/api"
        },
        "UserServer": {
            "type": "path",
            "url": "../UserServer/api"
        }
    },
    "require": {
        "laravel/framework": "4.2.*",
        "shinc/commons": "1.0",
        "shinc/auto-id-api": "1.0",
        "shinc/user-server-api": "1.0"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ]
    }
}
